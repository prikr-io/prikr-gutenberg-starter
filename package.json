{
  "name": "prikr-gutenberg-starter",
  "version": "1.0.0",
  "description": "WordPress Starter theme ready with Gutenberg blocks",
  "main": "src/js/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "dev": "node config/js/CleanUpDev.js && NODE_ENV=development TAILWIND_MODE=watch webpack serve --mode development",
    "start": "node config/js/CleanUpDev.js && NODE_ENV=production  webpack serve --mode production",
    "fonts": "node config/js/GoogleFonts.js"
  },
  "keywords": [
    "fse",
    "gutenberg",
    "wp",
    "wordpress"
  ],
  "author": "Prikr <info@prikr.io>",
  "license": "ISC",
