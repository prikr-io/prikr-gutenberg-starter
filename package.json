    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "glob": "^10.3.10",
